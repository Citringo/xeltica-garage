{
	"compilerOptions": {
		"module": "system",
		"target":"es5",
		"noImplicitAny": false,
		"removeComments": true,
		"preserveConstEnums": true,
		"outDir": "dist/js/",
		"sourceMap": false,
		"allowJs": true,
	},
	"include": [
		"src/ts/**/*"
	],
	"exclude": [
		"node_modules",
		"**/*.spec.ts"
	]
}